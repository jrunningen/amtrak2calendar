{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "indent": [
      true,
      "spaces",
      2
    ],
    "prettier": true,
    "trailing-comma": [
      false
    ],
    "no-console": true,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ]
}
